<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">

    <!--bold-->
    <font
            android:font="@font/pretendardbold"
            android:fontStyle="normal"
            android:fontWeight="1000"
            app:font="@font/pretendardbold"
            app:fontStyle="normal"
            app:fontWeight="1000"
            tools:targetApi="o" />

    <font
            android:font="@font/montserrat_alternates_bold"
            android:fontStyle="normal"
            android:fontWeight="1000"
            app:font="@font/montserrat_alternates_bold"
            app:fontStyle="normal"
            app:fontWeight="1000"
            tools:targetApi="o" />

    <!--medium-->
    <font
            android:font="@font/pretendardmedium"
            android:fontStyle="normal"
            android:fontWeight="500"
            app:font="@font/pretendardmedium"
            app:fontStyle="normal"
            app:fontWeight="500"
            tools:targetApi="o" />

    <font
            android:font="@font/montserrat_alternates_medium"
            android:fontStyle="normal"
            android:fontWeight="1000"
            app:font="@font/montserrat_alternates_bold"
            app:fontStyle="normal"
            app:fontWeight="1000"
            tools:targetApi="o" />


    <!--regular-->
    <font
            android:font="@font/pretendardregular"
            android:fontStyle="normal"
            android:fontWeight="300"
            app:font="@font/pretendardregular"
            app:fontStyle="normal"
            app:fontWeight="300"
            tools:targetApi="o" />

    <font
            android:font="@font/montserrat_alternates_regular"
            android:fontStyle="normal"
            android:fontWeight="1000"
            app:font="@font/montserrat_alternates_bold"
            app:fontStyle="normal"
            app:fontWeight="1000"
            tools:targetApi="o" />


</font-family>